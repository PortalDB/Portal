secondaryxlabel = Number of edges
axis-sql = select timestampdiff(year, f.year1, f.year2) as window, concat(round(sum(c.edges)/1000000,2),'M') as count from data_size c, (select distinct s.arg1 as year1, s.arg2 as year2 from (select q.query_id from query_op_map q, operation o where q.op_id=o.op_id and o.opType='Load' and o.arg1 like 'nGrams%%') as l, (select q.query_id, o.arg1, o.arg2 from query_op_map q, operation o where q.op_id=o.op_id and o.opType='Slice' and q.seqNum=2) as s, (select q.query_id from query_op_map q, operation o where q.op_id=o.op_id and o.opType='ConnectedComponents' and q.seqNum=3) as r where l.query_id=s.query_id and s.query_id=r.query_id) as f where str_to_date(c.date1, '%%Y-%%m-%%d') >= str_to_date(f.year1, '%%Y-%%m-%%d') and str_to_date(c.date2, '%%Y-%%m-%%d') <= str_to_date(f.year2, '%%Y-%%m-%%d') and c.dataset='nGrams' and f.year1='1720-01-01' group by f.year1, f.year2 order by 1;

