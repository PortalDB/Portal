[query-1]
type = line
name = components_dblp_vertices
xlabel = Number of representative graphs
ylabel = Runtime, seconds
sql = select h.window as "Number of RGs", avg(if(h.graphType='SG' and h.parts='None', h.runTime/1000, NULL)) as "RG", avg(if(h.graphType='OG' and h.parts='None', h.runTime/1000, NULL)) as "OG", avg(if(h.graphType='HG' and h.parts='None', h.runTime/1000, NULL)) as "HG", avg(if(h.graphType='SG' and h.parts='EdgePartition2D', h.runTime/1000, NULL)) as "RG_E2D", avg(if(h.graphType='OG' and h.parts='EdgePartition2D', h.runTime/1000, NULL)) as "OG_E2D", avg(if(h.graphType='HG' and h.parts='EdgePartition2D', h.runTime/1000, NULL)) as "HG_E2D" from (select e.runTime, e.graphType, f.window, f.parts from execution e, (select l.query_id, timestampdiff(year, s.arg1, s.arg2) as window, l.parts from (select q.query_id, o.opType, o.arg1, o.partitionS as parts from query_op_map q, operation o where q.op_id=o.op_id and o.opType='Load' and o.arg1 like 'dblp%%') as l, (select q.query_id, o.opType, o.arg1, o.arg2 from query_op_map q, operation o where q.op_id=o.op_id and o.opType='Slice' and q.seqNum=2) as s, (select q.query_id from query_op_map q, operation o where q.op_id=o.op_id and o.opType='ConnectedComponents' and q.seqNum=3) as r where l.query_id=s.query_id and s.query_id=r.query_id) as f where e.build_num=0 and e.query_id=f.query_id) as h group by h.window order by h.window;
secondaryxlabel = Number of edges
axis-sql = select timestampdiff(year, f.year1, f.year2) as window, concat(round(sum(c.edges)/1000000,2),'M') as count from data_size c, (select distinct s.arg1 as year1, s.arg2 as year2 from (select q.query_id from query_op_map q, operation o where q.op_id=o.op_id and o.opType='Load' and o.arg1 like 'dblp%%') as l, (select q.query_id, o.arg1, o.arg2 from query_op_map q, operation o where q.op_id=o.op_id and o.opType='Slice' and q.seqNum=2) as s, (select q.query_id from query_op_map q, operation o where q.op_id=o.op_id and o.opType='ConnectedComponents' and q.seqNum=3) as r where l.query_id=s.query_id and s.query_id=r.query_id) as f where str_to_date(c.date1, '%%Y-%%m-%%d') >= str_to_date(f.year1, '%%Y-%%m-%%d') and str_to_date(c.date2, '%%Y-%%m-%%d') <= str_to_date(f.year2, '%%Y-%%m-%%d') and c.dataset='dblp' group by f.year1, f.year2 order by 1;

