[configs]
main = edu.drexel.cs.dbgroup.graphxt.Driver
env = mesos
mesosConfig = --master mesos://master:5050 --conf spark.executor.uri=hdfs://master:9000/spark/spark-1.3.1-bin-hadoop2.6.tgz --executor-memory 6g --driver-memory 6g --conf spark.mesos.coarse=true target/scala-2.10/tgraph-assembly-1.0.jar
localConfig = --master local --jars lib/graphx-extensions_2.10-1.0.jar target/scala-2.10/temporal-graph-project_2.10-1.0.jar
ec2Config = --master spark://ec2-54-234-129-137.compute-1.amazonaws.com:7077 --jars lib/graphx-extensions_2.10-1.0.jar target/scala-2.10/temporal-graph-project_2.10-1.0.jar
clusterConfig = noCluster
buildNum = 2 
warm = 0
iterations = 3 
type = MG
data = hdfs://master:9000/data/nGrams
strategy = None,CanonicalRandomVertexCut,EdgePartition2D,NaiveTemporal,ConsecutiveTemporal,HybridRandomTemporal,Hybrid2DTemporal
numParts = 8,16,32

[test_queries]
Queries =
    G1_small --select 1815 1830 -r --count
    G1_large --select 1820 1860 -r --count
    G2_E --agg 50 existential -r --count
    G2_U --agg 50 universal -r --count
    G3_snapshot --getsnapshot 1993 -r --count
