main edu.drexel.cs.dbgroup.graphxt.Driver
env mesos
mesosConfig --master mesos://master:5050 target/scala-2.10/tgraph-assembly-1.0.jar --conf "spark.executor.uri=hdfs://master:9000/spark/spark-1.3.1-bin-hadoop2.6.tgz" --executor-memory 6g --conf spark.mesos.coarse=true
localConfig --master local --jars lib/graphx-extensions_2.10-1.0.jar target/scala-2.10/temporal-graph-project_2.10-1.0.jar
ec2Config --master spark://ec2-54-234-129-137.compute-1.amazonaws.com:7077 --jars lib/graphx-extensions_2.10-1.0.jar target/scala-2.10/temporal-graph-project_2.10-1.0.jar
clusterConfig noCluster
buildNum 1 
warm 0
iterations 5 
type SG
data hdfs://master:9000/data/dblp
AggE80by20 --agg 20 existential --count -p CanonicalRandomVertexCut 8
PrE16by8 --select 2000 2015 --agg 8 existential --pagerank 50 --count
